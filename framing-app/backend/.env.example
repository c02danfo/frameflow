# Framing app backend env (EXAMPLE)
# Copy to `framing-app/backend/.env` when running backend standalone.
# When running via docker-compose.production.yml, env is provided by Compose.

# Framing App Database
DB_HOST=framing-db
DB_PORT=5432
DB_NAME=framing_app
DB_USER=inventory_user
DB_PASSWORD=change_me

# Inventory Database (read-only)
INVENTORY_DB_HOST=inventory-db
INVENTORY_DB_PORT=5432
INVENTORY_DB_NAME=inventory_artyx
INVENTORY_DB_USER=inventory_user
INVENTORY_DB_PASSWORD=change_me

# Server
PORT=3011
NODE_ENV=development
SESSION_SECRET=CHANGE_ME_GENERATE_A_LONG_RANDOM_SECRET
JWT_SECRET=CHANGE_ME_GENERATE_A_LONG_RANDOM_SECRET
DOMAIN=frameflow.test

# Moms
MOMS_PERCENTAGE=25
