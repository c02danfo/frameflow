# Local/dev environment variables (EXAMPLE)
# Copy to `.env` for local development.
# Never commit `.env`.

# Shared secrets (must match across services for SSO)
SESSION_SECRET=CHANGE_ME_GENERATE_A_LONG_RANDOM_SECRET
JWT_SECRET=CHANGE_ME_GENERATE_A_LONG_RANDOM_SECRET

# Database passwords (used by docker-compose.production.yml)
AUTH_DB_PASSWORD=change_me
INVENTORY_DB_PASSWORD=change_me
FRAMING_DB_PASSWORD=change_me

# Base domain for redirects/cookies (recommended local dev)
# Example: frameflow.test -> inventory.frameflow.test, framing.frameflow.test
DOMAIN=frameflow.test
